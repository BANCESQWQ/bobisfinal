{
  "name": "joaco",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node --no-deprecation node_modules/@angular/cli/bin/ng serve",
    "start:dev": "node --no-warnings node_modules/@angular/cli/bin/ng serve",
    "dev": "ng serve --disable-deprecation-warnings",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:joaco": "node dist/joaco/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.11",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.9",
    "@angular/material": "^20.2.11",
    "@angular/platform-browser": "^20.3.0",
    "@angular/platform-server": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@angular/ssr": "^20.3.8",
    "@azure/msal-angular": "^4.0.21",
    "@azure/msal-browser": "^4.26.1",
    "chart.js": "^4.5.1",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "ng2-charts": "^8.0.0",
    "regression": "^2.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.8",
    "@angular/cli": "^20.3.11",
    "@angular/compiler-cli": "^20.3.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.2"
  }
}
